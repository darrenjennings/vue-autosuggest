{
  "name": "vue-autosuggest",
  "version": "0.0.2-beta.8",
  "description": "Vue autosuggest component.",
  "main": "dist/index.js",
  "engines": {
    "node": "> 4",
    "npm": "> 3"
  },
  "scripts": {
    "commit": "git-cz",
    "start": "webpack-dev-server --config docs/webpack.config.js",
    "build:docs": "NODE_ENV=production webpack --config docs/webpack.config.js",
    "build": "rm -rf dist && NODE_ENV=production webpack --config build/webpack.config.js",
    "test": "yarn jest",
    "report-coverage": "cat ./coverage/lcov.info | ./node_modules/.bin/codecov",
    "test:coverage": "yarn jest -- --coverage",
    "test:update": "yarn test -- --updateSnapshot --coverage",
    "add-contributor": "kcd-scripts contributors add",
    "setup": "yarn install"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Darren Jennings <dmjenn02@gmail.com> (http://www.darrenjennings.com/)",
  "license": "MIT",
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js"
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/educents/vue-autosuggest.git"
  },
  "bugs": {
    "url": "https://github.com/educents/vue-autosuggest/issues"
  },
  "homepage": "https://github.com/educents/vue-autosuggest#readme",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "yarn test"
    }
  },
  "devDependencies": {
    "babel-core": "6.26.0",
    "babel-jest": "21.2.0",
    "babel-loader": "7.1.2",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "babel-preset-env": "1.6.0",
    "codecov": "2.3.1",
    "codecov.io": "0.1.6",
    "commitizen": "2.9.6",
    "css-loader": "0.28.7",
    "cz-conventional-changelog": "2.0.0",
    "diffable-html": "2.1.0",
    "ghooks": "2.0.0",
    "jest": "21.2.1",
    "jest-serializer-html": "4.0.1",
    "kcd-scripts": "0.27.1",
    "vue": "2.5.2",
    "vue-jest": "1.0.0",
    "vue-loader": "13.3.0",
    "vue-server-renderer": "2.5.2",
    "vue-template-compiler": "2.5.2",
    "vue-template-es2015-compiler": "1.6.0",
    "vue-test-utils": "1.0.0-beta.4",
    "webpack": "3.7.1",
    "webpack-dev-server": "2.9.2",
    "webpack-merge": "4.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "verbose": true,
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-html"
    ],
    "mapCoverage": true
  },
  "dependencies": {}
}
